<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.fedoraproject.eclipse.packager"
      label="Fedora Packager for Eclipse"
      version="0.2.1.qualifier"
      provider-name="Fedoraproject.org">

   <description url="http://fedorahosted.org/eclipse-fedorapackager">
      Eclipse Fedora Packager is an Eclipse plugin helping 
      Fedora contributors interacting with Fedora infrastructure
      such as Koji, Bodhi and Fedora Git.
   </description>

   <copyright url="epl-v10.html">
      %copyright
